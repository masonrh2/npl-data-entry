{
    "env": {
        "browser": true,
        "es2021": true,
        "googleappsscript/googleappsscript": true,
        "jquery": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-trailing-spaces": "warn",
      "camelcase": "warn"
    },
    "plugins": [
      "html",
      "googleappsscript",
      "jquery"
  ]
}
